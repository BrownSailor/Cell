mod src;

use ios;

main(argc: int, argv: []str): int 
{

    num: int = 5;
    print("hello world");

    ? num > 5 
    {
        print("num is greater than 5");
    } | num < 5 
    {
        print("num is less than 5");
    } | 
    {
        print("num is equal to 5");
    }

    @ i: int = argc - 1, i >= 0, i-- 
    {
        print(argv[i]);
    }

    @ num > 0 
    {
        print(num);
        num--;
    }

    ret num;
}
